{
	"name": "wmui-style-guide",
	"version": "0.1.0",
	"description": "Wikimedia Design Style Guide, focus on Wikimedia User-Interface",
	"keywords": [
		"ui"
	],
	"homepage": "https://design.wikimedia.org/style-guide/",
	"repository": {
		"type": "git",
