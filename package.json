	},
	"private": true,
	"license": "MIT",
	"dependencies": {
		"fontfaceobserver": "2.1.0",
		"normalize.css": "8.0.1",
		"wikimedia-ui-base": "0.19.0"
	},
	"devDependencies": {
		"@lodder/grunt-postcss": "3.1.1",
		"autoprefixer": "10.2.6",
		"browserslist-config-wikimedia": "0.2.0",
		"cssnano": "5.0.6",
		"eslint-config-wikimedia": "0.20.0",
		"glob": "^8.0.3",
		"grunt": "1.6.1",
		"grunt-contrib-concat": "2.1.0",
		"grunt-contrib-uglify": "5.2.2",
		"grunt-contrib-watch": "1.1.0",
		"grunt-eslint": "24.3.0",
		"grunt-exec": "3.0.0",
		"grunt-replace": "2.0.2",
		"grunt-stylelint": "0.19.0",
		"pixrem": "5.0.0",
		"postcss": "8.4.31",
		"postcss-cssnext": "3.1.0",
		"postcss-custom-properties": "11.0.0",
		"postcss-import": "15.1.0",
		"sharp": "^0.32.6",
		"stylelint-config-wikimedia": "0.11.1",
		"svgo": "2.8.0"
	},
	"scripts": {
		"start": "grunt watch",
		"minify:svg": "svgo --config=.svgo.config.js --quiet --recursive --folder ./ --exclude principles-paper-ink.svg",
		"build:png": "node svg-to-png.mjs",
		"build": "npm run minify:svg && grunt & npm run build:png",
		"test": "grunt lint"
	}
